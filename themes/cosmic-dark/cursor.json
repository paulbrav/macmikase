{
  "colorTheme": "Default Dark Modern",
  "extension": null,
  "colors": {
    "background": "#141414",
    "foreground": "#c8c8c8",
    "accent": "#4096f5",
    "sidebar": "#0d0d0d",
    "terminal": "#141414",
    "error": "#f54040",
    "warning": "#f5f540"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6e6e6e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#4096f5"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#c8c8c8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#c8c8c8"
      }
    },
    {
      "name": "Markdown - Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#4096f5",
        "fontStyle": "bold"
      }
    }
  ]
}
