{
  "colorTheme": "Default Light Modern",
  "extension": null,
  "light": true,
  "colors": {
    "background": "#f0f0f0",
    "foreground": "#373737",
    "accent": "#3380d9",
    "sidebar": "#e6e6e6",
    "terminal": "#f0f0f0",
    "error": "#cc3333",
    "warning": "#b39900"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8a8a8a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#3380d9"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#22863a"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#6f42c1"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#373737"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#373737"
      }
    },
    {
      "name": "Markdown - Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3380d9",
        "fontStyle": "bold"
      }
    }
  ]
}
