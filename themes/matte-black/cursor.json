{
  "colorTheme": "Default Dark Modern",
  "extension": null,
  "colors": {
    "background": "#121212",
    "foreground": "#bebebe",
    "accent": "#ffc107",
    "sidebar": "#0a0a0a",
    "terminal": "#121212",
    "error": "#d35f5f",
    "warning": "#ffc107"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5c5c5c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#ffc107"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a5d6a7"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#64b5f6"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#e57373"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ba68c8"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#bebebe"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#bebebe"
      }
    },
    {
      "name": "Markdown - Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ffc107",
        "fontStyle": "bold"
      }
    }
  ]
}
