{
  "colorTheme": "Default Dark Modern",
  "extension": null,
  "colors": {
    "background": "#0c0d11",
    "foreground": "#e6e6e6",
    "accent": "#48b9c7",
    "sidebar": "#08090d",
    "terminal": "#0c0d11",
    "error": "#e95420",
    "warning": "#f6d32d"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a6c7a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#e95420"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#48b9c7"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#31ba96"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#7d57c1"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#e2c044"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Markdown - Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#e95420",
        "fontStyle": "bold"
      }
    }
  ]
}
